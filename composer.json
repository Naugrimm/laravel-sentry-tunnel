{
    "name": "asbiin/laravel-sentry-tunnel",
    "description": "Provides an endpoint to use the `tunnel`-parameter of the Sentry SDK",
    "keywords": [
        "laravel",
        "php",
        "sentry",
        "tunnel"
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/asbiin/laravel-sentry-tunnel/issues",
        "source": "https://github.com/asbiin/laravel-sentry-tunnel"
    },
    "authors": [
        {
            "name": "Alexis Saettler",
            "email": "alexis@saettler.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/http": "^10.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.8",
        "symplify/easy-coding-standard": "^10.1",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.1",
        "psalm/plugin-laravel": "^2.0",
        "phpstan/phpstan-strict-rules": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Naugrim\\LaravelSentryTunnel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SentryTunnel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naugrim\\LaravelSentryTunnel\\Provider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ocramius/package-versions": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
